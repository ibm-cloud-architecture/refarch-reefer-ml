### Cloud Pak for Data credentials
cp4d_user=<user>
cp4d_pwd=<password>
cp4d_base_url="<base url>"
cp4d_prediction_url="<prediction url>"

### Config specific to reeferTelemetries kafka topic
mp.messaging.incoming.reeferTelemetries.connector=liberty-kafka
mp.messaging.incoming.reeferTelemetries.group.id=scoring-telemetries-group
mp.messaging.incoming.reeferTelemetries.topic=reeferTelemetries
mp.messaging.incoming.reeferTelemetries.key.deserializer=org.apache.kafka.common.serialization.StringDeserializer
mp.messaging.incoming.reeferTelemetries.value.deserializer=org.apache.kafka.common.serialization.StringDeserializer

### Config specific to containers kafka topic
mp.messaging.outgoing.containers.connector=liberty-kafka
mp.messaging.outgoing.containers.group.id=scoring-containers-group
mp.messaging.outgoing.containers.topic=testOutput
mp.messaging.outgoing.containers.key.serializer=org.apache.kafka.common.serialization.StringSerializer
mp.messaging.outgoing.containers.value.serializer=org.apache.kafka.common.serialization.StringSerializer

### Config shared between all kafka connections
mp.messaging.connector.liberty-kafka.bootstrap.servers=<kafka bootstrap servers>
mp.messaging.connector.liberty-kafka.security.protocol=SASL_SSL
mp.messaging.connector.liberty-kafka.ssl.protocol=TLSv1.2
mp.messaging.connector.liberty-kafka.sasl.jaas.config=org.apache.kafka.common.security.plain.PlainLoginModule required username="<api key username>" password="<api key>";
mp.messaging.connector.liberty-kafka.sasl.mechanism=PLAIN